<h2>Organize your own co-working session</h2>
<%= form_for @event do |f| %>
<p>
<%= f.label :starts_at %>
<%= f.text_field :starts_at %>
</p>
<p>
<%= f.label :duration %>
<%= f.text_field :duration %>
</p>
<p>
<%= f.label :description %>
<%= f.text_area :description %>
</p>
<p>
<span class="inline">
<span class="buttonblock">
<%= f.submit "Create", :class=>"button" %>
</span>
</span>
</p>
<% end %>